{
  "name": "wdio-allure-ts",
  "version": "1.1.2",
  "description": "UI E2E testing util",
  "license": "MIT",
  "author": "Cloudinary <info@cloudinary.com>",
  "homepage": "http://cloudinary.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/cloudinary/wdio-allure-ts"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "keywords": [
    "cloudinary",
    "allure",
    "reporter",
    "webdriverio",
    "wdio",
    "wdio-plugin",
    "wdio-reporter",
    "test examples",
    "E2E UI",
    "examples"
  ],
  "scripts": {
    "tslint": "tslint -c tslint.json 'src/**/*.ts' -p ./ --fix",
    "clean": "rm -rf ./lib",
    "prettier": "prettier --write 'src/**/*.ts'; prettier --write 'src/test/**/*.ts'",
    "compile": "npm run clean && npm run prettier && npm run tslint && tsc",
    "test": "npm run test:compilation && npm run test:regression",
    "test:compilation": "npm run compile && git diff --exit-code ./lib",
    "test:regression": "npm run test:regression:chrome && npm run test:regression:firefox",
    "test:regression:chrome": "wdio src/test/wdio.chrome.conf.js --suite regression",
    "test:regression:firefox": "wdio src/test/wdio.firefox.conf.js --suite regression",
    "spec": "wdio src/test/wdio.dev.conf.js --spec",
    "start:sampleApp": "ws --spa src/test/sampleApp/index.html"
  },
  "dependencies": {
    "@applitools/eyes-sdk-core": "^5.20.2",
    "@applitools/eyes-webdriverio": "^5.8.1",
    "@types/adm-zip": "^0.4.32",
    "@types/mocha": "^5.2.7",
    "@types/request-promise-native": "^1.0.16",
    "@wdio/allure-reporter": "^5.12.1",
    "@wdio/sync": "^5.12.3",
    "adm-zip": "^0.4.13",
    "chai": "^4.2.0",
    "chalk": "^2.4.2",
    "request-promise-native": "^1.0.7"
  },
  "devDependencies": {
    "@types/chai": "^4.2.0",
    "@types/node": "^12.7.2",
    "@wdio/cli": "^5.12.5",
    "@wdio/local-runner": "^5.12.5",
    "@wdio/mocha-framework": "^5.12.1",
    "@wdio/selenium-standalone-service": "^5.12.1",
    "@wdio/spec-reporter": "^5.12.1",
    "local-web-server": "3.0.7",
    "prettier": "1.16.4",
    "tslint": "5.16.0",
    "tslint-config-airbnb": "5.11.1",
    "tslint-config-prettier": "1.18.0",
    "tslint-eslint-rules": "5.4.0",
    "tslint-microsoft-contrib": "6.0.0",
    "typescript": "^3.5.3",
    "webdriverio": "^5.12.5"
  }
}
