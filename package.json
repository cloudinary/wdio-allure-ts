{
  "name": "wdio-allure-ts",
  "version": "0.0.1",
  "description": "UI E2E testing util",
  "license": "MIT",
  "author": "Cloudinary <info@cloudinary.com>",
  "homepage": "http://cloudinary.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/cloudinary/wdio-allure-ts"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "keywords": [
    "cloudinary",
    "allure",
    "reporter",
    "webdriverio",
    "wdio",
    "wdio-plugin",
    "wdio-reporter",
    "test examples",
    "E2E UI",
    "examples"
  ],
  "scripts": {
    "tslint": "tslint -c tslint.json 'src/**/*.ts' -p ./ --fix",
    "clean": "rm -rf ./lib",
    "ts_compile": "tsc",
    "prepare": "run-s clean tslint ts_compile",
    "test": "run-s test:compilation start:sampleApp test:regression",
    "test:compilation": "run-s prepare; git diff --exit-code ./lib",
    "test:regression": "wdio ./test/wdio.conf.js --suite regression",
    "start:sampleApp": "ws --spa ./test/SampleApp/index.html > ./ws.log 2>&1 &"
  },
  "dependencies": {
    "chalk": "^2.4.1",
    "npm": "^6.1.0",
    "wdio-allure-reporter": "^0.6.2",
    "webdriverio": "^4.13.0",
    "chai": "^4.1.2"
  },
  "devDependencies": {
    "@types/chai": "^4.1.4",
    "@types/mocha": "^5.2.3",
    "@types/webdriverio": "^4.10.2",
    "dotenv": "^6.0.0",
    "local-web-server": "^2.5.2",
    "npm-run-all": "^4.1.3",
    "tslint": "^5.10.0",
    "tslint-config-airbnb": "^5.9.2",
    "tslint-config-prettier": "^1.13.0",
    "tslint-eslint-rules": "^5.3.1",
    "tslint-microsoft-contrib": "^5.0.3",
    "typescript": "^2.9.2",
    "wdio-mocha-framework": "^0.6.1",
    "wdio-selenium-standalone-service": "0.0.10",
    "wdio-spec-reporter": "^0.1.4",
    "wdio-typescript-service": "0.0.3",
    "webdriver-manager": "^12.0.6"
  }
}
