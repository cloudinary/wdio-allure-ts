{
  "name": "wdio-allure-ts",
  "version": "1.2.5",
  "description": "UI E2E testing util",
  "license": "MIT",
  "author": "Cloudinary <info@cloudinary.com>",
  "homepage": "http://cloudinary.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/cloudinary/wdio-allure-ts"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "bin": {
    "wdio-cli": "lib/helpers/runner.js"
  },
  "keywords": [
    "cloudinary",
    "allure",
    "reporter",
    "webdriverio",
    "wdio",
    "wdio-plugin",
    "wdio-reporter",
    "test examples",
    "E2E UI",
    "examples"
  ],
  "scripts": {
    "tslint": "tslint -c tslint.json 'src/**/*.ts' -p ./ --fix",
    "clean": "rm -rf ./lib",
    "prettier": "prettier --write 'src/**/*.ts'; prettier --write 'src/test/**/*.ts'",
    "prepare": "npm run clean && npm run prettier && npm run tslint && tsc && cp -R src/helpers/ lib/helpers",
    "compile": "npm run prettier && npm run tslint && tsc",
    "setChromeDriverVersion": "./src/scripts/setChromeDriverVersion.sh",
    "test": "npm run compile && npm run setChromeDriverVersion && wdio src/test/wdio.conf.js --suite regression",
    "spec": "wdio src/test/wdio.conf.js --spec",
    "start:sampleApp": "ws --spa src/test/sampleApp/index.html"
  },
  "dependencies": {
    "@applitools/eyes-sdk-core": "11.0.2",
    "@applitools/eyes-webdriverio": "5.12.0",
    "@types/request-promise-native": "^1.0.17",
    "@wdio/allure-reporter": "6.1.23",
    "@wdio/cli": "6.2.0",
    "@wdio/devtools-service": "6.1.24",
    "@wdio/local-runner": "6.2.0",
    "@wdio/mocha-framework": "6.2.0",
    "@wdio/selenium-standalone-service": "6.1.14",
    "@wdio/sync": "6.2.0",
    "adm-zip": "0.4.16",
    "allure-commandline": "2.13.0",
    "chai": "^4.2.0",
    "chalk": "4.1.0",
    "dotenv": "^8.2.0",
    "lodash": "^4.17.19",
    "request-promise-native": "^1.0.8",
    "wdio-docker-service": "2.4.0",
    "wdio-image-comparison-service": "1.13.6",
    "webdriverio": "6.2.0",
    "yargs": "15.4.1"
  },
  "devDependencies": {
    "@types/adm-zip": "0.4.33",
    "@types/chai": "^4.2.7",
    "@types/mocha": "8.0.0",
    "@types/node": "14.0.23",
    "husky": "^4.2.3",
    "lint-staged": "^10.2.11",
    "local-web-server": "4.2.1",
    "prettier": "2.0.5",
    "tslint": "6.1.2",
    "typescript": "3.9.6"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "git add"
    ]
  }
}
