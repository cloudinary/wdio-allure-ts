    "test examples",
    "E2E UI",
    "examples"
  ],
  "scripts": {
    "tslint": "tslint -c tslint.json 'src/**/*.ts' -p ./ --fix",
    "clean": "rm -rf ./lib",
    "ts_compile": "tsc",
    "build": "run-s clean tslint ts_compile",
    "test": "run-s build; git diff --exit-code ./lib"
  },
  "dependencies": {
    "chalk": "^2.4.1",
    "wdio-allure-reporter": "^0.6.1",
    "webdriverio": "^4.12.0"
  },
  "devDependencies": {
    "@types/webdriverio": "^4.10.0",
    "npm-run-all": "^4.1.3",
    "tslint": "^5.10.0",
    "tslint-config-airbnb": "^5.8.0",
    "tslint-config-prettier": "^1.12.0",
    "tslint-eslint-rules": "^5.2.0",
    "tslint-microsoft-contrib": "^5.0.3",
    "typescript": "^2.8.3"
  }
