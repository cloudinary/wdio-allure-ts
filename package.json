{
  "name": "wdio-allure-ts",
  "version": "0.0.1",
  "description": "UI E2E testing util",
  "license": "MIT",
  "author": "Cloudinary <info@cloudinary.com>",
  "homepage": "http://cloudinary.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/cloudinary/wdio-allure-ts"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "keywords": [
    "cloudinary",
    "allure",
    "reporter",
    "webdriverio",
    "wdio",
    "wdio-plugin",
    "wdio-reporter",
    "test examples",
    "E2E UI",
    "examples"
  ],
  "scripts": {
    "tslint": "tslint -c tslint.json 'src/**/*.ts' -p ./ --fix",
    "clean": "rm -rf ./lib",
    "ts_compile": "tsc",
    "prepare": "run-s clean tslint ts_compile",
    "test": "run-s prepare; git diff --exit-code ./lib",
    "test:regression": "wdio ./test/wdio.conf.js --spec ./lib/test/specs/ActionsTest.js",
    "start:sampleApp": "ws --spa ./test/SampleApp/index.html"
  },
  "dependencies": {
    "chalk": "^2.4.1",
    "wdio-allure-reporter": "^0.6.2",
    "webdriverio": "^4.13.0"
  },
  "devDependencies": {
    "@types/chai": "^4.1.4",
    "@types/mocha": "^5.2.3",
    "@types/webdriverio": "^4.10.2",
    "chai": "^4.1.2",
    "dotenv": "^6.0.0",
    "npm-run-all": "^4.1.3",
    "tslint": "^5.10.0",
    "tslint-config-airbnb": "^5.9.2",
    "tslint-config-prettier": "^1.13.0",
    "tslint-eslint-rules": "^5.3.1",
    "tslint-microsoft-contrib": "^5.0.3",
    "typescript": "^2.9.2",
    "wdio-mocha-framework": "^0.6.1",
    "wdio-spec-reporter": "^0.1.4",
    "wdio-typescript-service": "0.0.3"
  }
}
