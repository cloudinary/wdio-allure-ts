{
  "name": "wdio-allure-ts",
  "version": "0.0.1",
  "description": "WebdriverIO, Allure reporter and TypeScript wrapper for UI E2E testing",
  "license": "MIT",
  "author": "Felix Zilber <felix@cloudinary.com>",
  "homepage": "http://cloudinary.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/cloudinary/wdio-allure-ts"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "keywords": [
    "cloudinary",
    "allure",
    "reporter",
    "webdriverio",
    "wdio",
    "wdio-plugin",
