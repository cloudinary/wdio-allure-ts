{
  "name": "wdio-allure-ts",
  "version": "1.2.1",
  "description": "UI E2E testing util",
  "license": "MIT",
  "author": "Cloudinary <info@cloudinary.com>",
  "homepage": "http://cloudinary.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/cloudinary/wdio-allure-ts"
  },
  "main": "lib/index.js",
  "bin": {
    "wdio-cli": "lib/helpers/runner.js"
  },
  "types": "lib/index.d.ts",
  "keywords": [
    "cloudinary",
    "allure",
    "reporter",
    "webdriverio",
    "wdio",
    "wdio-plugin",
    "wdio-reporter",
    "test examples",
    "E2E UI",
    "examples"
  ],
  "scripts": {
    "tslint": "tslint -c tslint.json 'src/**/*.ts' -p ./ --fix",
    "clean": "rm -rf ./lib",
    "prettier": "prettier --write 'src/**/*.ts'; prettier --write 'src/test/**/*.ts'",
    "compile": "npm run clean && npm run prettier && npm run tslint && tsc && cp -R src/helpers/ lib/helpers",
    "setChromeDriverVersion": "./src/scripts/setChromeDriverVersion.sh",
    "test": "npm run compile && git diff --exit-code ./lib && npm run setChromeDriverVersion && wdio src/test/wdio.conf.js --suite regression",
    "spec": "wdio src/test/wdio.conf.js --spec",
    "start:sampleApp": "ws --spa src/test/sampleApp/index.html"
  },
  "dependencies": {
    "@applitools/eyes-sdk-core": "9.1.0",
    "@applitools/eyes-webdriverio": "5.9.23",
    "@types/request-promise-native": "^1.0.17",
    "@wdio/allure-reporter": "6.0.12",
    "@wdio/cli": "6.0.13",
    "@wdio/devtools-service": "6.0.12",
    "@wdio/local-runner": "6.0.13",
    "@wdio/mocha-framework": "6.0.13",
    "@wdio/selenium-standalone-service": "6.0.12",
    "@wdio/sync": " 6.0.12",
    "adm-zip": "0.4.14",
    "allure-commandline": "2.13.0",
    "chai": "^4.2.0",
    "chalk": "4.0.0",
    "dotenv": "^8.2.0",
    "lodash": "^4.17.15",
    "request-promise-native": "^1.0.8",
    "wdio-docker-service": "2.3.1",
    "wdio-image-comparison-service": "1.12.2",
    "webdriverio": "6.0.13",
    "yargs": "15.3.1"
  },
  "devDependencies": {
    "@types/adm-zip": "0.4.32",
    "@types/node": "13.11.0",
    "@types/mocha": "7.0.2",
    "@types/chai": "^4.2.7",
    "local-web-server": "4.0.0",
    "prettier": "2.0.3",
    "tslint": "6.1.1",
    "typescript": "3.8.3"
  }
}
